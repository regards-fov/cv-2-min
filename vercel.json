{
    "routes": [
        {
            "src": "/cv(/.*)?",
            "dest": "/app/index.html"
        },
        {
            "src": "/app(/.*)?",
            "dest": "/app/index.html"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/app/index.html"
        }
    ]
}